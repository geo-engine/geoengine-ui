{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "core": {
            "projectType": "library",
            "schematics": {
                "@schematics/angular:component": {
                    "changeDetection": "OnPush",
                    "inlineStyle": false,
                    "inlineTemplate": false,
                    "prefix": "geoengine",
                    "style": "scss"
                },
                "@schematics/angular:directive": {
                    "prefix": "geoengine"
                }
            },
            "root": "projects/core",
            "sourceRoot": "projects/core/src",
            "prefix": "geoengine",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "tsConfig": "projects/core/tsconfig.lib.json",
                        "project": "projects/core/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/core/tsconfig.lib.prod.json"
                        },
                        "development": {}
                    },
                    "defaultConfiguration": "production"
                },
                "test": {
                    "builder": "@angular/build:unit-test"
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": ["projects/core/**/*.ts", "projects/core/**/*.html"]
                    }
                }
            }
        },
        "gis": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "changeDetection": "OnPush",
                    "inlineStyle": false,
                    "inlineTemplate": false,
                    "prefix": "geoengine",
                    "style": "scss"
                },
                "@schematics/angular:directive": {
                    "prefix": "geoengine"
                }
            },
            "root": "projects/gis",
            "sourceRoot": "projects/gis/src",
            "prefix": "geoengine",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:application",
                    "options": {
                        "allowedCommonJsDependencies": [
                            "dagre-d3",
                            "json-stringify-pretty-compact",
                            "fast-json-stable-stringify",
                            "lerc",
                            "moment",
                            "xml-utils"
                        ],
                        "outputPath": {
                            "base": "dist/gis"
                        },
                        "index": "projects/gis/src/index.html",
                        "polyfills": [],
                        "tsConfig": "projects/gis/tsconfig.app.json",
                        "assets": [
                            "projects/gis/src/favicon.ico",
                            "projects/gis/src/assets",
                            {
                                "glob": "**/*",
                                "input": "projects/common/assets",
                                "output": "assets"
                            }
                        ],
                        "styles": [
                            "projects/gis/src/styles.scss",
                            "projects/common/assets/fonts/poppins/poppins.css",
                            "projects/common/assets/fonts/pacifico/pacifico.css",
                            "projects/common/assets/fonts/material-design-icons/material-icons.css",
                            "node_modules/ol/ol.css",
                            "./node_modules/vis-timeline/dist/vis-timeline-graph2d.min.css"
                        ],
                        "scripts": ["node_modules/vis-timeline/peer/esm/vis-timeline-graph2d.min.js"],
                        "extractLicenses": false,
                        "sourceMap": true,
                        "optimization": false,
                        "namedChunks": true,
                        "browser": "projects/gis/src/main.ts"
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "8mb",
                                    "maximumError": "10mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "100kb"
                                }
                            ]
                        },
                        "development": {}
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {},
                    "configurations": {
                        "production": {
                            "buildTarget": "gis:build:production"
                        },
                        "development": {
                            "buildTarget": "gis:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "buildTarget": "gis:build"
                    }
                },
                "test": {
                    "builder": "@angular/build:unit-test"
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": ["projects/gis/**/*.ts", "projects/gis/**/*.html"]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "projects/gis/e2e/protractor.conf.js"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "gis:serve:production"
                        },
                        "development": {
                            "devServerTarget": "gis:serve:development"
                        }
                    },
                    "defaultConfiguration": "development"
                }
            }
        },
