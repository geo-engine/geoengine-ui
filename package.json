{
    "name": "geoengine",
    "version": "0.7.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "ng-serve-local": "node --dns-result-order ipv4first ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy-local.conf.json --configuration development",
        "build": "npm run build-prod:core && npm run build-apps",
        "build:core": "ng build --configuration development core",
        "build-watch:core": "ng build --configuration development core --watch",
        "build-prod:core": "ng build --configuration production core",
        "build-apps": "npm run build-prod:gis && npm run build-prod:dashboards/data-atlas && npm run build-prod:dashboards/ebv-analyzer && npm run build-prod:dashboards/gfbio && npm run build-prod:dashboards/nfdi-gdo",
        "serve:dashboards/data-atlas:local": "npm run ng-serve-local data-atlas",
        "serve:dashboards/data-atlas:nightly": "ng serve --configuration development data-atlas --proxy-config proxy-data-atlas.conf.json",
        "serve:dashboards/ebv-analyzer:local": "npm run ng-serve-local ebv-analyzer",
        "serve:dashboards/ebv-analyzer:nightly": "ng serve --configuration development ebv-analyzer --proxy-config proxy-ebv-analyzer.conf.json",
        "serve:dashboards/gfbio:local": "npm run ng-serve-local gfbio",
        "serve:dashboards/gfbio:nightly": "ng serve --configuration development gfbio --proxy-config proxy-gfbio.conf.json",
        "serve:dashboards/nfdi-gdo:local": "npm run ng-serve-local nfdi-gdo",
        "serve:dashboards/nfdi-gdo:nightly": "ng serve --configuration development nfdi-gdo --proxy-config proxy-nfdi-gdo.conf.json",
        "serve:gis:local": "npm run ng-serve-local gis",
        "serve:gis:nightly": "ng serve --configuration development gis --proxy-config proxy-geoengine.conf.json",
        "build-prod:dashboards/data-atlas": "ng build --configuration production data-atlas",
        "build-prod:dashboards/ebv-analyzer": "ng build --configuration production ebv-analyzer",
        "build-prod:dashboards/gfbio": "ng build --configuration production gfbio",
        "build-prod:dashboards/nfdi-gdo": "ng build --configuration production nfdi-gdo",
        "build-prod:gis": "ng build --configuration production gis",
        "publish:core:patch": "version=patch scripts/publish-to-github.sh",
        "publish:core:minor": "version=minor scripts/publish-to-github.sh",
        "publish:core:major": "version=major scripts/publish-to-github.sh",
        "prettier": "prettier",
        "check": "prettier --check . && ng lint",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "postinstall": "ngcc"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^14.1.2",
        "@angular/cdk": "^14.1.2",
        "@angular/common": "^14.1.2",
        "@angular/compiler": "^14.1.2",
        "@angular/core": "^14.1.2",
        "@angular/flex-layout": "^14.0.0-beta.40",
        "@angular/forms": "^14.1.2",
        "@angular/material": "^14.1.2",
        "@angular/platform-browser": "^14.1.2",
        "@angular/platform-browser-dynamic": "^14.1.2",
        "@angular/router": "^14.1.2",
        "@egjs/hammerjs": "^2.0.17",
        "codemirror": "~5.65.7",
        "d3": "~7.6.1",
        "dagre": "~0.8.5",
        "dagre-d3": "~0.6.4",
        "flatgeobuf": "3.23.1",
        "immutable": "~4.1.0",
        "keycharm": "0.4.0",
        "moment": "~2.29.4",
        "ngx-color-picker": "~12.0.1",
        "ngx-mat-select-search": "~4.2.1",
        "ol": "~6.15.1",
        "ol-mapbox-style": "~8.2.1",
        "proj4": "~2.8.0",
        "propagating-hammerjs": "^2.0.1",
        "rxjs": "~7.5.6",
        "tslib": "^2.4.0",
        "uuid": "8.3.2",
        "vega": "~5.22.1",
        "vega-embed": "~6.21.0",
        "vega-lite": "~5.5.0",
        "vis-timeline": "7.7.0",
        "vis-data": "7.1.4",
        "vis-util": "5.0.3",
        "xss": "1.0.14",
        "zone.js": "~0.11.8"
    },
    "devDependencies": {
        "@angular-devkit/architect": "^0.1401.2",
        "@angular-devkit/build-angular": "^14.1.2",
        "@angular-eslint/builder": "^14.0.2",
        "@angular-eslint/eslint-plugin": "^14.0.2",
        "@angular-eslint/eslint-plugin-template": "^14.0.2",
        "@angular-eslint/schematics": "^14.0.2",
        "@angular-eslint/template-parser": "^14.0.2",
        "@angular/cli": "^14.1.2",
        "@angular/compiler-cli": "^14.1.2",
        "@angular/language-service": "^14.1.2",
        "@types/codemirror": "~5.60.5",
        "@types/d3": "~7.4.0",
        "@types/dagre": "^0.7.47",
        "@types/dagre-d3": "^0.6.3",
        "@types/jasmine": "~4.0.3",
        "@types/jasminewd2": "~2.0.10",
        "@types/node": "^18.7.6",
        "@types/ol": "^6.5.3",
        "@types/proj4": "^2.5.2",
        "@typescript-eslint/eslint-plugin": "~5.33.1",
        "@typescript-eslint/parser": "~5.44.0",
        "eslint": "^8.22.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jsdoc": "39.3.6",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-prettier": "^4.2.1",
        "jasmine-core": "~4.3.0",
        "jasmine-spec-reporter": "~7.0.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.1.1",
        "karma-coverage-istanbul-reporter": "~3.0.3",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.0.0",
        "ng-packagr": "^14.1.0",
        "@nrwl/devkit": "// remove this when <https://github.com/nrwl/nx/issues/11653> is resolved",
        "@nrwl/devkit": "14.5.7",
        "prettier": "2.7.1",
        "protractor": "~7.0.0",
        "puppeteer": "~16.1.1",
        "ts-node": "~10.9.1",
        "typescript": "~4.7.4"
    }
}
