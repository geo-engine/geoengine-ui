{
    "name": "geoengine",
    "version": "0.7.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "ng-serve-local": "node --dns-result-order ipv4first ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy-local.conf.json --configuration development",
        "build": "npm run build-prod:common && npm run build-prod:core && npm run build-apps",
        "build:common": "ng build --configuration development common",
        "build:core": "ng build --configuration development core",
        "build-watch:common": "ng build --configuration development common --watch",
        "build-prod:common": "ng build --configuration production common",
        "build-watch:core": "ng build --configuration development core --watch",
        "build-prod:core": "ng build --configuration production core",
        "build-apps": "npm run build-prod:gis && npm run build-prod:dashboards/data-atlas && npm run build-prod:dashboards/ebv-analyzer && npm run build-prod:dashboards/gfbio && npm run build-prod:dashboards/nfdi-portal-demo",
        "serve:dashboards/data-atlas:local": "npm run ng-serve-local data-atlas",
        "serve:dashboards/data-atlas:nightly": "ng serve --configuration development data-atlas --proxy-config proxy-data-atlas.conf.json",
        "serve:dashboards/ebv-analyzer:local": "npm run ng-serve-local ebv-analyzer",
        "serve:dashboards/ebv-analyzer:nightly": "ng serve --configuration development ebv-analyzer --proxy-config proxy-ebv-analyzer.conf.json",
        "serve:dashboards/ecometrics:local": "npm run ng-serve-local ecometrics",
        "serve:dashboards/ecometrics:nightly": "ng serve --configuration development ecometrics --proxy-config proxy-ecometrics.conf.json",
        "serve:dashboards/esg-indicator-service:local": "npm run ng-serve-local esg-indicator-service",
        "serve:dashboards/esg-indicator-service:nightly": "ng serve --configuration development esg-indicator-service --proxy-config proxy-esg-indicator-service.conf.json",
        "serve:dashboards/gfbio:local": "npm run ng-serve-local gfbio",
        "serve:dashboards/gfbio:nightly": "ng serve --configuration development gfbio --proxy-config proxy-gfbio.conf.json",
        "serve:dashboards/nfdi-portal-demo:local": "npm run ng-serve-local nfdi-portal-demo",
        "serve:dashboards/nfdi-portal-demo:nightly": "ng serve --configuration development nfdi-portal-demo --proxy-config proxy-nfdi-portal-demo.conf.json",
        "serve:gis:local": "npm run ng-serve-local gis",
        "serve:gis:nightly": "ng serve --configuration development gis --proxy-config proxy-geoengine.conf.json",
        "serve:manager:local": "npm run ng-serve-local manager",
        "serve:manager:nightly": "ng serve --configuration development manager --proxy-config proxy-geoengine.conf.json",
        "build-prod:dashboards/data-atlas": "ng build --configuration production data-atlas",
        "build-prod:dashboards/ebv-analyzer": "ng build --configuration production ebv-analyzer",
        "build-prod:dashboards/ecometrics": "ng build --configuration production ecometrics",
        "build-prod:dashboards/esg-indicator-service": "ng build --configuration production esg-indicator-service",
        "build-prod:dashboards/gfbio": "ng build --configuration production gfbio",
        "build-prod:dashboards/nfdi-portal-demo": "ng build --configuration production nfdi-portal-demo",
        "build-prod:gis": "ng build --configuration production gis",
        "build-prod:manager": "ng build --configuration production manager",
        "publish:core:patch": "version=patch scripts/publish-to-github.sh",
        "publish:core:minor": "version=minor scripts/publish-to-github.sh",
        "publish:core:major": "version=major scripts/publish-to-github.sh",
        "prettier": "prettier",
        "check": "prettier --check . && ng lint",
        "test": "ng test --karma-config=karma.conf.js --watch=false --browsers=ChromeHeadless",
        "test:ci": "ng test --karma-config=karma.conf.js --watch=false --no-progress --browsers=ChromeHeadlessNoSandbox",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^19.2.10",
        "@angular/cdk": "^19.2.15",
        "@angular/common": "^19.2.10",
        "@angular/compiler": "^19.2.10",
        "@angular/core": "^19.2.10",
        "@angular/forms": "^19.2.10",
        "@angular/material": "^19.2.15",
        "@angular/platform-browser": "^19.2.10",
        "@angular/platform-browser-dynamic": "^19.2.10",
        "@angular/router": "^19.2.10",
        "@egjs/hammerjs": "^2.0.17",
        "@geoengine/openapi-client": "0.0.23",
        "codemirror": "~5.65.18",
        "d3": "~7.9.0",
        "dagre": "~0.8.5",
        "dagre-d3": "~0.6.4",
        "flatgeobuf": "4.0.1",
        "immutable": "~5.1.2",
        "keycharm": "0.4.0",
        "moment": "~2.30.1",
        "ngx-color-picker": "~19.0.0",
        "ngx-mat-select-search": "~8.0.1",
        "ol": "~10.5.0",
        "ol-mapbox-style": "~12.6.0",
        "proj4": "~2.15.0",
        "propagating-hammerjs": "^2.0.1",
        "rxjs": "^7.4.0",
        "uuid": "9.0.1",
        "vega": "~5.31.0",
        "vega-embed": "~6.27.1",
        "vega-lite": "~5.23.0",
        "vis-data": "7.1.9",
        "vis-timeline": "7.7.3",
        "vis-util": "5.0.7",
        "xss": "1.0.15",
        "zone.js": "~0.15.0"
    },
    "devDependencies": {
        "@angular-devkit/architect": "^0.1902.11",
        "@angular-devkit/build-angular": "^19.2.11",
        "@angular-eslint/builder": "^19.3.0",
        "@angular-eslint/eslint-plugin": "^19.3.0",
        "@angular-eslint/eslint-plugin-template": "^19.3.0",
        "@angular-eslint/schematics": "^19.3.0",
        "@angular-eslint/template-parser": "^19.3.0",
        "@angular/cli": "^19.2.11",
        "@angular/compiler-cli": "^19.2.10",
        "@angular/language-service": "^19.2.10",
        "@types/arcgis-rest-api": "^10.4.8",
        "@types/codemirror": "~5.60.15",
        "@types/d3": "~7.4.3",
        "@types/dagre": "^0.7.52",
        "@types/dagre-d3": "^0.6.6",
        "@types/jasmine": "~5.1.8",
        "@types/jasminewd2": "~2.0.13",
        "@types/node": "^22.15.16",
        "@types/proj4": "^2.5.6",
        "@types/topojson-specification": "^1.0.5",
        "@typescript-eslint/eslint-plugin": "^8.32.0",
        "@typescript-eslint/parser": "^8.32.0",
        "eslint": "^9.26.0",
        "eslint-config-prettier": "^10.1.3",
        "eslint-plugin-import": "2.31.0",
        "eslint-plugin-jsdoc": "50.6.11",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-prettier": "^5.4.0",
        "jasmine-core": "~5.7.1",
        "karma": "~6.4.4",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.1",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "ng-packagr": "^19.2.2",
        "prettier": "3.5.3",
        "puppeteer": "~24.8.2",
        "ts-node": "~10.9.2",
        "typescript": "5.8.3"
    }
}
