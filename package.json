{
    "name": "wave",
    "version": "3.0.2",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "build": "npm run build-prod:core && npm run build-apps",
        "build:core": "ng build --configuration development wave-core-new",
        "build-watch:core": "ng build --configuration development wave-core-new --watch",
        "build-prod:core": "ng build --configuration production wave-core-new",
        "build-apps": "npm run build-prod:geoengine && npm run build-prod:data-atlas && npm run build-prod:gfbio",
        "serve:geoengine:local": "ng serve --configuration development geoengine-app --proxy-config proxy-local.conf.json",
        "serve:geoengine:nightly": "ng serve --configuration development geoengine-app --proxy-config proxy-geoengine.conf.json",
        "serve:data-atlas:local": "ng serve --configuration development data-atlas-app --proxy-config proxy-local.conf.json",
        "serve:data-atlas:nightly": "ng serve --configuration development data-atlas-app --proxy-config proxy-data-atlas.conf.json",
        "serve:gfbio:local": "ng serve --configuration development gfbio-app --proxy-config proxy-local.conf.json",
        "serve:gfbio:nightly": "ng serve --configuration development gfbio-app --proxy-config proxy-gfbio.conf.json",
        "build-prod:data-atlas": "ng build --configuration production data-atlas-app",
        "build-prod:geoengine": "ng build --configuration production geoengine-app",
        "build-prod:gfbio": "ng build --configuration production gfbio-app",
        "publish:core:patch": "version=patch scripts/publish-to-github.sh",
        "publish:core:minor": "version=minor scripts/publish-to-github.sh",
        "publish:core:major": "version=major scripts/publish-to-github.sh",
        "prettier": "prettier",
        "check": "prettier --check . && ng lint",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~12.1.4",
        "@angular/cdk": "~12.1.4",
        "@angular/common": "~12.1.4",
        "@angular/compiler": "~12.1.4",
        "@angular/core": "~12.1.4",
        "@angular/flex-layout": "~12.0.0-beta.34",
        "@angular/forms": "~12.1.4",
        "@angular/material": "~12.1.4",
        "@angular/platform-browser": "~12.1.4",
        "@angular/platform-browser-dynamic": "~12.1.4",
        "@angular/router": "~12.1.4",
        "codemirror": "~5.58.1",
        "d3": "~6.2.0",
        "dagre": "~0.8.5",
        "dagre-d3": "~0.6.4",
        "hellojs": "~1.18.6",
        "html2canvas": "~1.0.0-rc.5",
        "immutable": "~4.0.0-rc.12",
        "moment": "~2.29.1",
        "ngx-color-picker": "~11.0.0",
        "ol": "~6.8.1",
        "papaparse": "~5.3.0",
        "proj4": "~2.6.2",
        "rxjs": "~6.6.3",
        "tslib": "^2.0.0",
        "vega": "~5.20.2",
        "vega-embed": "~6.18.2",
        "vega-lite": "~5.1.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/architect": "^0.1201.4",
        "@angular-devkit/build-angular": "~12.1.4",
        "@angular-eslint/builder": "^12.3.1",
        "@angular-eslint/eslint-plugin": "^12.3.1",
        "@angular-eslint/eslint-plugin-template": "^12.3.1",
        "@angular-eslint/schematics": "^12.3.1",
        "@angular-eslint/template-parser": "^12.3.1",
        "@angular/cli": "~12.1.4",
        "@angular/compiler-cli": "~12.1.4",
        "@angular/language-service": "~12.1.4",
        "@types/codemirror": "~0.0.87",
        "@types/d3": "~5.16.3",
        "@types/dagre": "^0.7.44",
        "@types/dagre-d3": "^0.6.2",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "@types/ol": "^6.4.2",
        "@types/papaparse": "~4.5.7",
        "@types/proj4": "^2.5.0",
        "@typescript-eslint/eslint-plugin": "~4.29.0",
        "@typescript-eslint/parser": "~4.29.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "2.23.4",
        "eslint-plugin-jsdoc": "36.0.6",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-prettier": "^3.4.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~6.0.0",
        "karma": "~6.3.4",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "~1.5.0",
        "ng-packagr": "~12.1.2",
        "prettier": "2.2.1",
        "protractor": "~7.0.0",
        "puppeteer": "~9.1.1",
        "ts-node": "~8.3.0",
        "typescript": "~4.3.5"
    }
}
